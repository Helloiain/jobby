@import 'nib'

  *
    overflow auto

  body 
    font 14px "Lucida Grande", Helvetica, Arial, sans-serif
    margin 0

  header
    display flex
    align-items flex-end
    background-color #212121
    color #fff
    height 150px

  a
    text-decoration none
    color #e5306e
      

  .container
    width 90%
    margin 0 auto
  
  .search-container
    display flex

  footer 
    background-color #212121
    color #fff
    margin-top 1rem

  